<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

 <androidx.constraintlayout.widget.Guideline
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:orientation="vertical"
     android:id="@+id/guideline_v_02"
     app:layout_constraintGuide_percent=".02"
     />
 <androidx.constraintlayout.widget.Guideline
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:orientation="vertical"
     android:id="@+id/guideline_v_30"
     app:layout_constraintGuide_percent=".30"
     />
 <androidx.constraintlayout.widget.Guideline
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:orientation="vertical"
     android:id="@+id/guideline_v_35"
     app:layout_constraintGuide_percent=".35"
     />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/guideline_v_40"
        app:layout_constraintGuide_percent=".40"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/guideline_v_60"
        app:layout_constraintGuide_percent=".60"
        />
 <androidx.constraintlayout.widget.Guideline
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:orientation="vertical"
     android:id="@+id/guideline_v_65"
     app:layout_constraintGuide_percent=".65"
     />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/guideline_v_70"
        app:layout_constraintGuide_percent=".70"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/guideline_v_98"
        app:layout_constraintGuide_percent=".98"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_02"
        app:layout_constraintGuide_percent=".02"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_25"
        app:layout_constraintGuide_percent=".25"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_35"
        app:layout_constraintGuide_percent=".35"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_40"
        app:layout_constraintGuide_percent=".40"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_55"
        app:layout_constraintGuide_percent=".55"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_60"
        app:layout_constraintGuide_percent=".60"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_65"
        app:layout_constraintGuide_percent=".65"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_80"
        app:layout_constraintGuide_percent=".80"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_85"
        app:layout_constraintGuide_percent=".85"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/guideline_h_95"
        app:layout_constraintGuide_percent=".95"
        />
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/igView1"
        android:src="@drawable/ig_man"
        app:layout_constraintTop_toTopOf="@id/guideline_h_02"
        app:layout_constraintStart_toStartOf="@id/guideline_v_02"
        app:layout_constraintEnd_toStartOf="@id/guideline_v_30"
        app:layout_constraintBottom_toTopOf="@id/guideline_h_25"
        android:background="#33691E"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/igView2"
        android:src="@drawable/ig_man"
        app:layout_constraintStart_toStartOf="@id/guideline_v_40"
        app:layout_constraintTop_toTopOf="@id/guideline_h_02"
        app:layout_constraintEnd_toStartOf="@id/guideline_v_60"
        app:layout_constraintBottom_toTopOf="@id/guideline_h_25"
        android:background="#0091EA"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/igView3"
        android:src="@drawable/ig_man"
        app:layout_constraintEnd_toEndOf="@id/guideline_v_98"
        app:layout_constraintTop_toTopOf="@id/guideline_h_02"
        app:layout_constraintStart_toStartOf="@id/guideline_v_70"
        app:layout_constraintBottom_toTopOf="@id/guideline_h_25"
        android:background="#D50000"
        />


    <ImageView
        android:id="@+id/igView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ig_left"
        android:background="#FF6F00"
        app:layout_constraintEnd_toEndOf="@id/guideline_v_35"
        app:layout_constraintTop_toTopOf="@id/guideline_h_35"
        app:layout_constraintBottom_toTopOf="@id/guideline_h_60"
        app:layout_constraintStart_toStartOf="@id/guideline_v_02"
        app:tint="#1A307E"/>

    <ImageView
        android:id="@+id/igView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ig_right"
        android:background="#00C853"
        app:layout_constraintTop_toBottomOf="@id/guideline_h_35"
        app:layout_constraintStart_toStartOf="@id/guideline_v_65"
        app:layout_constraintEnd_toEndOf="@id/guideline_v_98"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h_60"
        />

    <ImageView
        android:id="@+id/igView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#A700D5"
        android:src="@drawable/ig_swap"
        app:layout_constraintStart_toStartOf="@+id/guideline_v_30"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h_40"
        app:layout_constraintBottom_toTopOf="@id/guideline_h_55"
        app:layout_constraintEnd_toStartOf="@id/guideline_v_70"
        />

    <ImageView
        android:id="@+id/igView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00CED5"
        android:src="@drawable/ig_button"
        app:layout_constraintStart_toStartOf="@+id/guideline_v_02"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h_65"
        app:layout_constraintBottom_toTopOf="@id/guideline_h_80"
        app:layout_constraintEnd_toStartOf="@id/guideline_v_60"
        />

    <ImageView
        android:id="@+id/igView8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#E91A237E"
        android:src="@drawable/ig_button"
        app:layout_constraintStart_toStartOf="@+id/guideline_v_02"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h_85"
        app:layout_constraintBottom_toTopOf="@id/guideline_h_95"
        app:layout_constraintEnd_toStartOf="@id/guideline_v_40"
        />


</androidx.constraintlayout.widget.ConstraintLayout>